{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "remote": {
    "urls": [
      "http://localhost:8081/*",
      "http://127.0.0.1:8081/*"
    ]
  },
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "core:window:allow-set-badge-count",
    "core:window:allow-toggle-maximize",
    "dialog:default",
    "opener:default",
    "websocket:default"
  ]
}
