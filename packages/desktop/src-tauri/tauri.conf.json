{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Paseo",
  "version": "0.1.13",
  "identifier": "dev.paseo.desktop",
  "build": {
    "frontendDist": "../../app/dist",
    "devUrl": "http://localhost:8081"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Paseo",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true,
        "trafficLightPosition": {
          "x": 16,
          "y": 22
        }
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "entitlements": "Entitlements.plist",
      "infoPlist": "Info.plist"
    }
  }
}
