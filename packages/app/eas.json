{
  "cli": {
    "version": ">= 14.0.0"
  },
  "build": {
    "debug": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "debug",
      "env": {
        "APP_VARIANT": "debug"
      },
      "android": {
        "gradleCommand": ":app:assembleDebug"
      }
    },
    "fast": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "fast",
      "env": {
        "APP_VARIANT": "fast"
      },
      "android": {
        "gradleCommand": ":app:assembleDebugOptimized"
      }
    },
    "prod": {
      "channel": "production",
      "env": {
        "APP_VARIANT": "prod"
      }
    }
  },
  "submit": {
    "prod": {}
  }
}
